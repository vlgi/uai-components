{
  "name": "uai-components",
  "version": "4.0.1",
  "scripts": {
    "dev": "vite dev --host --port 5000",
    "storybook": "storybook dev -p 6006",
    "preview": "vite preview",
    "build": "vite build && npm run package",
    "build-storybook": "svelte-kit sync && storybook build",
    "package": "svelte-kit sync && svelte-package",
    "package:watch": "svelte-kit sync && svelte-package --watch",
    "check-all": "npm install && npm run lint && npm run format-check && npm run check && npm run test",
    "format-check": "prettier --check .",
    "lint-fix": "eslint --fix .",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check",
    "check:watch": "svelte-kit sync && svelte-check --watch",
    "lint": "svelte-kit sync && eslint .",
    "format": "prettier --write .",
    "test": "svelte-kit sync && vitest run src",
    "test:watch": "vitest src"
  },
  "description": "Svelte component package",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vlgi/uai-components.git"
  },
  "keywords": [
    "svelte",
    "components",
    "ui",
    "library",
    "package",
    "sveltekit",
    "vite",
    "storybook"
  ],
  "author": "vlgi",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/vlgi/uai-components/issues"
  },
  "homepage": "https://github.com/vlgi/uai-components#readme",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./styles": "./dist/styles/index.scss"
  },
  "svelte": "./dist/index.js",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*",
    "!dist/**/*.stories.*"
  ],
  "peerDependencies": {
    "sass": "^1.62.1",
    "svelte": "^3.54.0"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^7.0.18",
    "@storybook/addon-essentials": "^7.0.18",
    "@storybook/addon-interactions": "^7.0.18",
    "@storybook/blocks": "^7.0.18",
    "@storybook/jest": "^0.1.0",
    "@storybook/svelte": "^7.0.18",
    "@storybook/sveltekit": "^7.0.18",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.5.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^2.1.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@tsconfig/svelte": "^4.0.1",
    "@types/hammerjs": "^2.0.41",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-svelte": "^2.26.0",
    "jsdom": "^21.1.1",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.62.1",
    "storybook": "^7.0.18",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "svelte-eslint-parser": "^0.27.0",
    "svelte-preprocess": "^5.0.3",
    "tslib": "^2.4.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.0",
    "vitest": "^0.25.3"
  },
  "dependencies": {
    "@emoji-mart/data": "^1.1.1",
    "@iconify/svelte": "^3.1.3",
    "@iconify/types": "^2.0.0",
    "@types/cleave.js": "^1.4.6",
    "cleave.js": "^1.6.0",
    "fuse.js": "^6.6.2",
    "hammerjs": "^2.0.8",
    "remark-gfm": "^3.0.1",
    "svelte-dnd-action": "0.9.24",
    "svelte-awesome-color-picker": "2.4.6"
  }
}
